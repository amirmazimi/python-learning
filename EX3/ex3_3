def robot(commands):
    i = 0
    j = 0

    for x in commands:
        if x == "n":
           j += 1
        elif x == "e":
            i += 1
        elif x == "s":
            j -= 1
        elif x == "w":
            i -= 1
    if (j == 2 and i == 3) or (j == 3 and i == -4):
        return True
    else:
        return False
